public class Patient {
    private String firstName;
    private String lastName;
    private String DoB;
    private CoronaVaccineRecord Record;
    //Constructor
    public Patient(String firstName, String lastName, String DoB, CoronaVaccineRecord Record){
        this.firstName = firstName;
        this.lastName = lastName;
        this.DoB = DoB;
        this.Record = Record;

    }
    //Gettor
    public String getFirstName(){
        return firstName;
    }
    public String getlastName(){
        return lastName;
    }
    public String DoB(){
        return DoB;
    }
    public CoronaVaccineRecord Record(){ return Record;}
    //SETTOR
    public void setFirstName(String firstName){
        this.firstName = firstName;
    }
    public void setLastName(String lastName){
        this.lastName = lastName;
    }
    public void setDoB(String DoB){
        this.DoB = DoB;
    }
    public String toString(){
        return firstName +  " "  + lastName + " was born in " + DoB + " " + " and their record states that " + Record;
    }
}
